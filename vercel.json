{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "outputDirectory": "dist",
  "buildCommand": "npm run build",
  "redirects": [
    { "source": "/certificacao/", "destination": "/certificacao", "permanent": true },
    { "source": "/empresas/", "destination": "/empresas", "permanent": true },
    { "source": "/contacto/", "destination": "/contacto", "permanent": true },
    { "source": "/sobre-nos/", "destination": "/sobre-nos", "permanent": true },
    { "source": "/about", "destination": "/sobre-nos", "permanent": true },
    { "source": "/about/", "destination": "/sobre-nos", "permanent": true },
    { "source": "/politica-privacidade/", "destination": "/politica-privacidade", "permanent": true },
    { "source": "/politica-de-privacidade", "destination": "/politica-privacidade", "permanent": true },
    { "source": "/politica-de-privacidade/", "destination": "/politica-privacidade", "permanent": true },
    { "source": "/termos-uso/", "destination": "/termos-uso", "permanent": true },
    { "source": "/termos-de-uso", "destination": "/termos-uso", "permanent": true },
    { "source": "/termos-de-uso/", "destination": "/termos-uso", "permanent": true },
    { "source": "/certificacaco", "destination": "/certificacao", "permanent": true },
    { "source": "/certificacaco/", "destination": "/certificacao", "permanent": true }
  ]
}
