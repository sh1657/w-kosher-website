{
  "version": 2,
  "builds": [
    {
      "src": "netlify-deploy/**",
      "use": "@vercel/static"
    },
    {
      "src": "admin/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/netlify-deploy/$1"
    }
  ]
}
